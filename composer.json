            "stuartcusackie\\StatamicSEO\\": "src"
        }
    },
    "authors": [
        {
            "name": "Stuart Cusack",
            "email": "stuart@stuartcusack.ie",
            "homepage": "https://stuartcusack.ie",
            "role": "President"
        }
